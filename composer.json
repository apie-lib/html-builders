{
  "name": "apie/html-builders",
  "description": "Composer package of the apie library: html builders",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\HtmlBuilders\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\HtmlBuilders\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\HtmlBuilders\\Enums\\ActionDefinitionVariant",
      "Apie\\HtmlBuilders\\Enums\\LayoutEnum",
      "Apie\\HtmlBuilders\\ValueObjects\\FormName"
    ]
  },
  "require": {
    "php": ">=8.3",
    "apie/common": "self.version",
    "apie/core": "self.version",
    "ramsey/uuid": "^4.7.6",
    "symfony/mime":  "^7.2"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.3",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "conflict": {
    "phpstan/phpdoc-parser": "2.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}